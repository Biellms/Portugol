programa {
	
	funcao inicio() {

		inteiro a,b

		a = 10
		b = 10
		
		escreva(" As diferenças dos valores ",a," e ",b)
		escreva("\n A é maior que B?", a>b)
		escreva("\n A é menor que B?", a<b)
		escreva("\n A é diferente de B?", a!=b)
		escreva("\n A é igual ao B?", a==b)
		
	}
}
