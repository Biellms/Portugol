programa {
	
	funcao inicio() {

		inteiro tab, x = 1, resultado, op

		escreva("\n Digite um valor: ")
		leia(tab)

		faca {
			resultado = tab * x
			escreva("\n ",tab," X ",x," = ",resultado)
			x++
		} enquanto (x <= 10)
	}
}
